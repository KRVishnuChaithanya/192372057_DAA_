def first_palindromic_string(words):
    for word in words:
        if word == word[::-1]:  
            return word
    return ""  

print(first_palindromic_string(["abc", "car", "ada", "racecar", "cool"]))  
print(first_palindromic_string(["notapalindrome", "racecar"]))  
print(first_palindromic_string(["hello", "world"]))  
